<%= @menus.each do |menu|%>
    <div class="card" style="width: 18rem;">
    <%= image_tag menu.picture, class:"card-img-top"%>
    <div class="card-body">
        <h5 class="card-title"><%=menu.name%></h5>
        <p class="card-text"><%=menu.description%></p>
        <p class="card-text">$<%=menu.price%>
        <%= link_to "Show more", menu_path(menu.id), class:"btn btn-primary btn-right" %></p>
    </div>
    </div>
<% end %>
